@Article{Hurtado2019,
  author                 = {Hurtado, Fernando Manuel Matias and Pinto, Maísa de Siqueira and Oliveira, Perla Novais de and Riaño-Pachón, Diego Mauricio and Inocente, Laura Beatriz and Carrer, Helaine},
